{
    "builds": [{
        "src": "authOrg/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "authOrg/wsgi.py"
        }
    ],

    "env": {
    "DB_NAME": "@DB_NAME",
    "DB_USER": "@DB_USER",
    "DB_PASSWORD": "@DB_PASSWORD",
    "DB_HOST": "@DB_HOST",
    "DB_PORT": "@DB_PORT"
  }
}